{
  "rules": {
    ".read": false,
    ".write": true,
    "log": {
      ".indexOn": ["mac"]
    },
    "alert": {
      ".indexOn": ["mac"]
    },
    "ping": {
      ".indexOn": ["mac", "timestamp", "gcp_timestamp", "lookupKey"]
    },
    "module": {
      ".indexOn": ["mac", "timestamp", "gcp_timestamp"]
    }
  }
}